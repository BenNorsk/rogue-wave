<div class="container">
<%= simple_form_for([:user, @boat]) do |f| %>
  <%= f.input :name%>
  <%= f.input :description %>
  <%= f.input :price_per_day %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.simple_fields_for :address do |a| %>
  <%=   a.input :street_address %>
  <%=   a.input :city  %>
  <%=   a.input :country %>
  <% end %>
  <%= f.submit %>
<% end %>
</div>
